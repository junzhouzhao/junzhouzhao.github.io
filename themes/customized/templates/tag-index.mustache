<section id="content" role="main">
    <div id="outline-container-sec-1" class="row" style="padding-top: 70px">
            <div class="container center-block">
                <div class="col-md-2"></div>
                <div class="col-md-8 col-sm-12">
                    <div class="panel panel-default" id="text-1">
                        <div class="panel-heading">
                            <h3 id="sec-1" class="panel-title">Tag Index</h3>
                        </div>
                        <ul class="list-group">
                            {{#tags}}
                                <li class="list-group-item title"><a href="{{tag-uri}}">{{tag-name}} ({{count}})</a></li>
                            {{/tags}}
                        </ul>
                    </div>
                    <div class="col-md-2"></div>
                </div>
            </div>
        </div>
</section>
